using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication9
{
    class Program
    {
        static void Main(string[] args)
        {
            double x1, x2, y1, y2, d, x, y;
            Console.Write("Введите координаты x и y первой точки: ");
            x1 = double.Parse(Console.ReadLine());
            y1 = double.Parse(Console.ReadLine());
            Console.Write("Введите координаты x и y второй точки: ");
            x2 = double.Parse(Console.ReadLine());
            y2 = double.Parse(Console.ReadLine());
            if (x1 > x2)
            { x = x1 - x2; }
            else
            { x = x2 - x1; }
            y = x * x;
            x = y;
            if (y1 > y2)
            { y = y1 - y2; }
            else
            { y = y2 - y1; }
            y1 = y * y;
            y = y1;
            d = Math.Sqrt(x + y);
            Console.Write("Расстояние на плоскости между данными точками равно ");
            Console.WriteLine(d);
            Console.ReadKey();
        }
    }
}
